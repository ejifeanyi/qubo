{
	"compilerOptions": {
		"lib": ["ES2015"],
		"outDir": "./dist",
		"target": "es2015",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"moduleResolution": "node",
		"types": ["node"]
	},
	"exclude": ["node_modules"],
	"include": ["src/**/*"]
}
